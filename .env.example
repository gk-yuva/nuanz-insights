# Example Environment Configuration
# Copy this file to .env and replace with your actual API credentials

# Upstox API Configuration
UPSTOX_API_KEY=your_upstox_api_key_here
UPSTOX_API_SECRET=your_upstox_api_secret_here  
UPSTOX_ACCESS_TOKEN=your_upstox_access_token_here
UPSTOX_BASE_URL=https://api.upstox.com/v2

# MF API Configuration (Free service - no credentials required)
MF_API_BASE_URL=https://api.mfapi.in/mf

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=true